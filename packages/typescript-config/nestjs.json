{
	"$schema": "https://json.schemastore.org/tsconfig",
	"extends": "./base.json",
	"compilerOptions": {
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"forceConsistentCasingInFileNames": true,
		"incremental": true,
		"noFallthroughCasesInSwitch": true,
		"removeComments": true,
		"skipLibCheck": true,
		"sourceMap": true
	}
}
